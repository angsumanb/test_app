    <% if @testrun.testcases.any? %>
      <h3>Testcases (<%= @testrun.testcases.count %>)</h3>
      <table id="table1" class="table table-striped table-bordered table-hover table-condensed">
      <tr>
        <th>Testresult Id</th>
        <th>Testcase Id</th>
        <th>Testrun Id</th>
        <th>Title</th>
        <th>Type</th>
        <th>Priority</th>
        <th>Status</th>
        <th>Change Status</th>
        <th>Change Status</th>
      </tr>
       <% @testrun.testresults.each do |t| %>
      <tr>
          
        <td><%= t.id %></td>
        <td><%= t.testcase.id %></td>
        <td><%= t.testrun.id %></td>
        <td><%= t.testcase.title %></td>
        <td><%= t.testcase.testtype %></td>
        <td><%= t.testcase.priority %></td>
        <td><span id=testresult><%= t.status %></span></td>
        <td><%= button_to "t.status", { testcaseId: t.testcase.id, testrunId: @testrun, controller: "testresults", action: "makePass"} , {remote: true, class: "btn btn-small", id: "button1" } %></td>
        <td><button class="btn btn-small" id="button2">Hello</button></td>
        
       <% end %>
      </tr>
      </table>
    <% end %>
