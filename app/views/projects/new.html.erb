<% provide(:title, 'Create new project') %>
<h1>Create new project</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@project) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_area :description, size: "40*20" %>

</br>
      <%= f.submit "Create new project", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
