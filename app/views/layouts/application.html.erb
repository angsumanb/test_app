<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= javascript_include_tag "testruns" %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %> 


</head>
  <body>

    <%= render 'layouts/header' %>
    <div class="container-fluid">
      <div class="row-fluid">
      	<div class="span3 bs-docs-sidebar ">
        <ul class="nav nav-pills nav-stacked">
        	
          <li class="active"><a href="#global"><i class="icon-chevron-right"></i> Navigate to</a></li>
           <li><%= link_to  root_path do %>
           	<i class="icon-home"></i> Home
                		<%end%>
           </li>
          <li><%= link_to    dashboard_path do %>
          	<i class="icon-th-large"></i> Dashboard
                		<%end%>
          </li>
          	<li class="active"><a href='#'><i class="icon-chevron-right"></i> Actions</a></li>
          
          	 
          </li>
          
          <%= yield :sidebar %>
          
        </ul>
      </div>
       
       <div class="span9">
         <div class="body">
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>

      <%= yield %>
      
    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>
         </div>
       </div>
    </div>
  </body>
</html>

